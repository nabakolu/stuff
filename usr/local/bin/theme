#!/bin/bash


awesome-client command 'awesome.restart()'
pidof st | xargs kill -s USR1
wal_steam
oomox-cli /opt/oomox/scripted_colors/xresources/xresources-reverse -o oomox-xresources-reverse -t ~/.themes
